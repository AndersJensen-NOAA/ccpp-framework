<?xml version='1.0' encoding='utf-8'?>
<scheme module="cnvc90">
  <subroutine name="cnvc90_init" />
  <subroutine name="cnvc90_run">
    <var>
      <name>convective_cloud_switch  ??</name>
      <units>none</units>
      <id>clstp</id>
      <rank>0</rank>
      <type>real</type>
      <description>switch for saving convective clouds</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_dimension</name>
      <units>index</units>
      <id>ix</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>instantaneous_rainfall_amount</name>
      <units>m</units>
      <id>rn</id>
      <rank>1</rank>
      <type>real</type>
      <description>convective rainfall</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_cloud_base</name>
      <units>index</units>
      <id>kbot</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at cloud base</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_cloud_top</name>
      <units>index</units>
      <id>ktop</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at cloud top</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>km</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of vertical layers</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_at_interface</name>
      <units>Pa</units>
      <id>prsi</id>
      <rank>2</rank>
      <type>real</type>
      <description>interface pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>convective_rainfall_accumulated_thus_far  ??</name>
      <units>m</units>
      <id>acv</id>
      <rank>1</rank>
      <type>real</type>
      <description>convective rainfall accumulated thus far</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>smallest_cloud_base_vertical_index_encountered_thus_far  ??</name>
      <units>index</units>
      <id>acvb</id>
      <rank>1</rank>
      <type>real</type>
      <description>smallest cloud base vertical index encountered thus far</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>largest_cloud_top_vertical_index_encountered_thus_far  ??</name>
      <units>index</units>
      <id>acvt</id>
      <rank>1</rank>
      <type>real</type>
      <description>largest cloud top vertical index encountered thus far</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>fraction_of_convective_cloud  ??</name>
      <units>none</units>
      <id>cv</id>
      <rank>1</rank>
      <type>real</type>
      <description>fraction of convective cloud</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>pressure_at_bottom_of_convective_cloud  ??</name>
      <units>Pa</units>
      <id>cvb</id>
      <rank>1</rank>
      <type>real</type>
      <description>pressure at bottom of convective cloud</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>pressure_at_top_of_convective_cloud  ??</name>
      <units>Pa</units>
      <id>cvt</id>
      <rank>1</rank>
      <type>real</type>
      <description>pressure at top of convective cloud</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="cnvc90_finalize" />
</scheme>
