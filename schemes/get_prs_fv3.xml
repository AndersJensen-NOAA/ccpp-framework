<?xml version='1.0' encoding='utf-8'?>
<scheme module="get_prs_fv3">
  <subroutine name="get_prs_fv3_init" />
  <subroutine name="get_prs_fv3_run">
    <var>
      <name>horizontal_dimension</name>
      <units>index</units>
      <id>ix</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>levs</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of vertical layers</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>geopotential_at_interface</name>
      <units>m2 s-2</units>
      <id>phii</id>
      <rank>2</rank>
      <type>real</type>
      <description>interface geopotential</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_at_interface</name>
      <units>Pa</units>
      <id>prsi</id>
      <rank>2</rank>
      <type>real</type>
      <description>interface pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature</name>
      <units>K</units>
      <id>tgrs</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer temperature</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>water_vapor_specific_humidity</name>
      <units>kg kg-1</units>
      <id>qgrs1</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer specific humidity of water vapor</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_difference_between_midlayers</name>
      <units>Pa</units>
      <id>del</id>
      <rank>2</rank>
      <type>real</type>
      <description>difference between mid-layer pressures</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>geopotential_difference_between_midlayers_divided_by_midlayer virtual_temperature</name>
      <units>m2 s-2 K-1</units>
      <id>del_gz</id>
      <rank>2</rank>
      <type>real</type>
      <description>difference between mid-layer geopotentials divided by mid-layer virtual temperature</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="get_prs_fv3_finalize" />
</scheme>
