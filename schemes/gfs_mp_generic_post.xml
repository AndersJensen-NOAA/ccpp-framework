      <description>horizontal loop extent, start at 1</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_dimension</name>
      <units>index</units>
      <id>ix</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>levs</id>
      <rank>0</rank>
      <type>integer</type>
      <description>vertical layer dimension</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_dynamics</name>
      <units>s</units>
