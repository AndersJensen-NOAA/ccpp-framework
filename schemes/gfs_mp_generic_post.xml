      <description>logical flag for model physics diagnostics</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>logical_flag_for_3D_diagnostics</name>
      <units>flag</units>
      <id>ldiag3d</id>
      <rank>0</rank>
      <type>logical</type>
      <description>logical flag for 3D diagnostics</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
