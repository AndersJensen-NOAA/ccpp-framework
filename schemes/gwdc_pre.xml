<?xml version='1.0' encoding='utf-8'?>
<scheme module="gwdc_pre">
  <subroutine name="gwdc_pre_init" />
  <subroutine name="gwdc_pre_run">
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>multiplication_factors_for_convective_gravity_wave_drag</name>
      <units>none</units>
      <id>cgwf</id>
      <rank>1</rank>
      <type>real</type>
      <description>multiplication factors for convective gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>grid_size_in_x??</name>
      <units>m</units>
      <id>dx</id>
      <rank>1</rank>
      <type>real</type>
      <description>grid size in zonal direction</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>work_array_1??</name>
      <units>none</units>
      <id>work1</id>
      <rank>1</rank>
      <type>real</type>
      <description>work array #1</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>work_array_2??</name>
      <units>none</units>
      <id>work2</id>
      <rank>1</rank>
      <type>real</type>
      <description>work array #2</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>characteristic_grid_scale</name>
      <units>m</units>
      <id>dlength</id>
      <rank>1</rank>
      <type>real</type>
      <description>characteristic grid scale</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>cloud_area_fraction</name>
      <units>frac</units>
      <id>cldf</id>
      <rank>1</rank>
      <type>real</type>
      <description>fraction of grid box area in which updrafts occur</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>levs</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of vertical layers</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_cloud_base</name>
      <units>index</units>
      <id>kbot</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at cloud base</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_cloud_top</name>
      <units>index</units>
      <id>ktop</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at cloud top</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_physics</name>
      <units>s</units>
      <id>dtp</id>
      <rank>0</rank>
      <type>real</type>
      <description>physics time step</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature_updated_by_physics</name>
      <units>m s-1</units>
      <id>gt0</id>
      <rank>2</rank>
      <type>real</type>
      <description>updated air temperature</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature_before_deep_convection??</name>
      <units>K</units>
      <id>gt0_no_convec</id>
      <rank>2</rank>
      <type>real</type>
      <description>air temperature before considering tendency due to deep convection</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_difference_between_midlayers</name>
      <units>Pa</units>
      <id>del</id>
      <rank>2</rank>
      <type>real</type>
      <description>difference between mid-layer pressures</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>maximum_column_heating_rate</name>
      <units>K s-1</units>
      <id>cumabs</id>
      <rank>1</rank>
      <type>real</type>
      <description>maximum heating rate in column</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="gwdc_pre_finalize" />
</scheme>
