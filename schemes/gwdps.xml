<?xml version='1.0' encoding='utf-8'?>
<scheme module="gwdps">
  <subroutine name="gwdps_init" />
  <subroutine name="gwdps_run">
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_dimension</name>
      <units>index</units>
      <id>ix</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>iy</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>km</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of vertical layers</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_y_wind_due_to_model_physics</name>
      <units>m s-2</units>
      <id>A</id>
      <rank>2</rank>
      <type>real</type>
      <description>meridional wind tendency due to model physics</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_x_wind_due_to_model_physics</name>
      <units>m s-2</units>
      <id>B</id>
      <rank>2</rank>
      <type>real</type>
      <description>zonal wind tendency due to model physics</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>tendency_of_air_temperature_due_to_model_physics</name>
      <units>K s-1</units>
      <id>C</id>
      <rank>2</rank>
      <type>real</type>
      <description>air temperature tendency due to model physics</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>x_wind</name>
      <units>m s-1</units>
      <id>u1</id>
      <rank>2</rank>
      <type>real</type>
      <description>zonal wind</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>y_wind</name>
      <units>m s-1</units>
      <id>v1</id>
      <rank>2</rank>
      <type>real</type>
      <description>meridional wind</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature</name>
      <units>K</units>
      <id>t1</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer temperature</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>water_vapor_specific_humidity</name>
      <units>kg kg-1</units>
      <id>q1</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer specific humidity of water vapor</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_index_at_top_of_atmosphere_boundary_layer</name>
      <units>index</units>
      <id>kpbl</id>
      <rank>1</rank>
      <type>integer</type>
      <description>vertical index at top atmospheric boundary layer</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_at_interface</name>
      <units>Pa</units>
      <id>prsi</id>
      <rank>2</rank>
      <type>real</type>
      <description>interface pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_difference_between_midlayers</name>
      <units>Pa</units>
      <id>del</id>
      <rank>2</rank>
      <type>real</type>
      <description>difference between mid-layer pressures</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure</name>
      <units>Pa</units>
      <id>prsl</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>dimensionless_exner_function</name>
      <units>none</units>
      <id>prslk</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer Exner function</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>geopotential_at_interface</name>
      <units>m2 s-2</units>
      <id>phii</id>
      <rank>2</rank>
      <type>real</type>
      <description>interface geopotential</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>geopotential</name>
      <units>m2 s-2</units>
      <id>phil</id>
      <rank>2</rank>
      <type>real</type>
      <description>mid-layer geopotential</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_physics</name>
      <units>s</units>
      <id>deltim</id>
      <rank>0</rank>
      <type>real</type>
      <description>physics time step</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>index_of_time_step</name>
      <units>index</units>
      <id>kdt</id>
      <rank>0</rank>
      <type>integer</type>
      <description>current time step index</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>standard_deviation_of_subgrid_orography</name>
      <units>m</units>
      <id>hprime</id>
      <rank>1</rank>
      <type>real</type>
      <description>standard deviation of subgrid orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>convexity_of_subgrid_orography</name>
      <units>none</units>
      <id>oc</id>
      <rank>1</rank>
      <type>real</type>
      <description>convexity of subgrid orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>asymmetry_of_subgrid_orography</name>
      <units>none</units>
      <id>oa4</id>
      <rank>2</rank>
      <type>real</type>
      <description>asymmetry of subgrid orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>fraction_of_grid_box_with_subgrid_orography_higher_than_critical_height</name>
      <units>frac</units>
      <id>clx4</id>
      <rank>2</rank>
      <type>real</type>
      <description>horizontal fraction of grid box covered by subgrid orography higher than critical height</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>angle_from_east_of_maximum_subgrid_orographic_variations</name>
      <units>degrees</units>
      <id>theta</id>
      <rank>1</rank>
      <type>real</type>
      <description>angle with respect to east of maximum subgrid orographic variations</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>slope_of_subgrid_orography</name>
      <units>none</units>
      <id>sigma</id>
      <rank>1</rank>
      <type>real</type>
      <description>slope of subgrid orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>anisotropy_of_subgrid_orography</name>
      <units>none</units>
      <id>gamma</id>
      <rank>1</rank>
      <type>real</type>
      <description>anisotropy of subgrid orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>maximum_subgrid_orography</name>
      <units>m</units>
      <id>elvmax</id>
      <rank>1</rank>
      <type>real</type>
      <description>maximum of subgrid orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>instantaneous_x_stress_due_to_gravity_wave_drag</name>
      <units>Pa</units>
      <id>dusfc</id>
      <rank>1</rank>
      <type>real</type>
      <description>zonal surface stress due to orographic gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>instantaneous_y_stress_due_to_gravity_wave_drag</name>
      <units>Pa</units>
      <id>dvsfc</id>
      <rank>1</rank>
      <type>real</type>
      <description>meridional surface stress due to orographic gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>gravitational_acceleration</name>
      <units>m s-2</units>
      <id>g</id>
      <rank>0</rank>
      <type>real</type>
      <description>gravitational acceleration</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>specific_heat_of_dry_air_at_constant_pressure</name>
      <units>J kg-1 K-1</units>
      <id>cp</id>
      <rank>0</rank>
      <type>real</type>
      <description>specific heat of dry air at constant pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>gas_constant_dry_air</name>
      <units>J kg-1 K-1</units>
      <id>rd</id>
      <rank>0</rank>
      <type>real</type>
      <description>ideal gas constant for dry air</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>gas_constant_water_vapor</name>
      <units>J kg-1 K-1</units>
      <id>rv</id>
      <rank>0</rank>
      <type>real</type>
      <description>ideal gas constant for water vapor</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>number_of_equatorial_longitude_points</name>
      <units>count</units>
      <id>imx</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of longitude points along the equator</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>number_of_statistical_measures_of_subgrid_orography</name>
      <units>count</units>
      <id>nmtvr</id>
      <rank>0</rank>
      <type>integer</type>
      <description>number of statistical measures of subgrid orography</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>multiplication_factors_for_mountain_blocking_and_orographic_gravity_wave_drag</name>
      <units>none</units>
      <id>cdmbgwd</id>
      <rank>1</rank>
      <type>real</type>
      <description>multiplic. factors for (1) mountain blocking drag coeff. and (2) ref. level orographic gravity wave drag</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>mpi_rank</name>
      <units>index</units>
      <id>me</id>
      <rank>0</rank>
      <type>integer</type>
      <description>rank of the current MPI task</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_print</name>
      <units>flag</units>
      <id>lprnt</id>
      <rank>0</rank>
      <type>logical</type>
      <description>flag for debugging printouts</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_index_of_printed_column</name>
      <units>index</units>
      <id>ipr</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal index of column used in debugging printouts</description>
      <kind>default</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="gwdps_finalize" />
</scheme>
