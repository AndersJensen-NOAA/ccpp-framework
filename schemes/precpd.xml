<?xml version='1.0' encoding='utf-8'?>
<scheme module="precpd">
  <subroutine name="precpd_init" />
  <subroutine name="precpd_run">
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent, start at 1</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_dimension</name>
      <units>index</units>
      <id>ix</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal dimension</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>vertical_dimension</name>
      <units>index</units>
      <id>km</id>
      <rank>0</rank>
      <type>integer</type>
      <description>vertical layer dimension</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>time_step_for_physics</name>
      <units>s</units>
      <id>dt</id>
      <rank>0</rank>
      <type>real</type>
      <description>physics time step</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure_difference_between_midlayers</name>
      <units>Pa</units>
      <id>del</id>
      <rank>2</rank>
      <type>real</type>
      <description>pressure level thickness</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_pressure</name>
      <units>Pa</units>
      <id>prsl</id>
      <rank>2</rank>
      <type>real</type>
      <description>layer mean pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>water_vapor_specific_humidity</name>
      <units>kg kg-1</units>
      <id>q</id>
      <rank>2</rank>
      <type>real</type>
      <description>water vapor specific humidity</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>cloud_condensed_water_specific_humidity</name>
      <units>kg kg-1</units>
      <id>cwm</id>
      <rank>2</rank>
      <type>real</type>
      <description>cloud condensed water specific humidity</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature</name>
      <units>K</units>
      <id>t</id>
      <rank>2</rank>
      <type>real</type>
      <description>layer mean air temperature</description>
      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>rainfall_amount</name>
      <units>m</units>
      <id>rn</id>
      <rank>1</rank>
      <type>real</type>
      <description>large scale rainfall amount</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>ratio_of_snowfall_to_rainfall</name>
      <units>frac</units>
      <id>sr</id>
      <rank>1</rank>
      <type>real</type>
      <description>ratio of snowfall to large-scale rainfall</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>rain_water_path</name>
      <units>kg m-3</units>
      <id>rainp</id>
      <rank>2</rank>
      <type>real</type>
      <description>rainwater path</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>critical_relative_humidity</name>
      <units>1</units>
      <id>u00k</id>
      <rank>2</rank>
      <type>real</type>
      <description>critical relative humidity</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>coefficient_from_cloud_ice_to_snow</name>
      <units>1</units>
      <id>psautco</id>
      <rank>0</rank>
      <type>real</type>
      <description>conversion coefficient from cloud ice to snow</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>coefficient_from_cloud_water_to_rain</name>
      <units>1</units>
      <id>prautco</id>
      <rank>0</rank>
      <type>real</type>
      <description>conversion coefficient from cloud water to rain</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>coefficient_for_evaporation_of_rainfall</name>
      <units>1</units>
      <id>evpco</id>
      <rank>0</rank>
      <type>real</type>
      <description>coefficient for evaporation of rainfall</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>cloud_condensed_water_conversion_threshold</name>
      <units>1</units>
      <id>wminco</id>
      <rank>0</rank>
      <type>real</type>
      <description>conversion coefficient from cloud liquid and ice to precipitation</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>coefficient_for_grid</name>
      <units>frac</units>
      <id>wk1</id>
      <rank>1</rank>
      <type>real</type>
      <description>grid coefficient</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_print</name>
      <units>flag</units>
      <id>lprnt</id>
      <rank>0</rank>
      <type>logical</type>
      <description>flag for printing diagnostics to output</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>horizontal_index_of_printed_column</name>
      <units>index</units>
      <id>jpr</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal index of printed column</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
  </subroutine>
  <subroutine name="precpd_finalize" />
</scheme>
