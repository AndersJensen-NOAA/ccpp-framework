<?xml version='1.0' encoding='utf-8'?>
<scheme module="sfc_diag">
  <subroutine name="sfc_diag_init" />
  <subroutine name="sfc_diag_finalize" />
  <subroutine name="sfc_diag_run">
    <var>
      <name>horizontal_loop_extent</name>
      <units>index</units>
      <id>im</id>
      <rank>0</rank>
      <type>integer</type>
      <description>horizontal loop extent, start at 1</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>surface_air_pressure</name>
      <units>Pa</units>
      <id>ps</id>
      <rank>1</rank>
      <type>real</type>
      <description>surface pressure</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>x_wind_at_lowest_layer</name>
      <units>m s-1</units>
      <id>u1</id>
      <rank>1</rank>
      <type>real</type>
      <description>x component of 1st model layer wind</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>y_wind_at_lowest_layer</name>
      <units>m s-1</units>
      <id>v1</id>
      <rank>1</rank>
      <type>real</type>
      <description>y component of 1st model layer wind</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>air_temperature_at_lowest_layer</name>
      <units>K</units>
      <id>t1</id>
      <rank>1</rank>
      <type>real</type>
      <description>1st model layer air temperature</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>specific_humidity_at_lowest_layer</name>
      <units>kg kg-1</units>
      <id>q1</id>
      <rank>1</rank>
      <type>real</type>
      <description>1st model layer specific humidity</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>surface_skin_temperature</name>
      <units>K</units>
      <id>tskin</id>
      <rank>1</rank>
      <type>real</type>
      <description>surface skin temperature</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>surface_specific_humidity</name>
      <units>kg kg-1</units>
      <id>qsurf</id>
      <rank>1</rank>
      <type>real</type>
      <description>surface specific humidity</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>ratio_of_wind_at_lowest_layer_and_wind_at_10m</name>
      <units>ratio</units>
      <id>f10m</id>
      <rank>1</rank>
      <type>real</type>
      <description>ratio of fm10 and fm</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>x_wind_at_10m</name>
      <units>m s-1</units>
      <id>u10m</id>
      <rank>1</rank>
      <type>real</type>
      <description>x component of wind at 10 m</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>y_wind_at_10m</name>
      <units>m s-1</units>
      <id>v10m</id>
      <rank>1</rank>
      <type>real</type>
      <description>y component of wind at 10 m</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>temperature_at_2m</name>
      <units>K</units>
      <id>t2m</id>
      <rank>1</rank>
      <type>real</type>
      <description>temperature at 2 m</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>specific_humidity_at_2m</name>
      <units>kg kg-1</units>
      <id>q2m</id>
      <rank>1</rank>
      <type>real</type>
      <description>specific humidity at 2 m</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>ratio_of_exner_function_between_midlayer_and_interface_at_lowest_layer</name>
      <units>ratio</units>
      <id>prslki</id>
      <rank>1</rank>
      <type>real</type>
      <description>Exner function ratio bt midlayer and interface at 1st layer</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>kinematic_surface_upward_latent_heat_flux</name>
      <units>kg kg-1 m s-1</units>
      <id>evap</id>
      <rank>1</rank>
      <type>real</type>
      <description>surface upward evaporation flux</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>Monin-Obukhov_similarity_function_for_momentum</name>
      <units>none</units>
      <id>fm</id>
      <rank>1</rank>
      <type>real</type>
      <description>Monin-Obukhov similarity parameter for momentum</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>Monin-Obukhov_similarity_function_for_heat</name>
      <units>none</units>
      <id>fh</id>
      <rank>1</rank>
      <type>real</type>
      <description>Monin-Obukhov similarity parameter for heat</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>Monin-Obukhov_similarity_function_for_momentum_at_10m</name>
      <units>none</units>
      <id>fm10</id>
      <rank>1</rank>
      <type>real</type>
      <description>Monin-Obukhov similarity parameter for momentum</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>Monin-Obukhov_similarity_function_for_heat_at_2m</name>
      <units>none</units>
      <id>fh2</id>
      <rank>1</rank>
      <type>real</type>
      <description>Monin-Obukhov similarity parameter for heat</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
  </subroutine>
</scheme>
