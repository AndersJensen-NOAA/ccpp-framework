      <description>horizontal loop extent, start at 1</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>sea_land_ice_mask</name>
      <units>flag</units>
      <id>islimsk</id>
      <rank>1</rank>
      <type>integer</type>
      <description>landmask: sea/land/ice=0/1/2</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>orography</name>
      <units />
      <id>oro</id>
      <rank>1</rank>
      <type>real</type>
      <description>orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>orography_unfiltered</name>
      <units />
      <id>oro_uf</id>
      <rank>1</rank>
      <type>real</type>
      <description>unfiltered orographyo</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>flag_for_nsstm_run</name>
      <units>flag</units>
      <id>nstf_name1</id>
      <rank>1</rank>
