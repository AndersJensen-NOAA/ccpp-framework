      <description>landmask: sea/land/ice=0/1/2</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>sea_surface_foundation_temperature</name>
      <units>K</units>
      <id>tref</id>
      <rank>1</rank>
      <type>real</type>
