      <kind>kind_phys</kind>
      <intent>inout</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>mean_change_over_depth_in_sea_water_temperature</name>
      <units>K</units>
      <id>dtzm</id>
      <rank>1</rank>
      <type>real</type>
      <description>mean of dT(z)  (zsea1 to zsea2)</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>surface_skin_temperature</name>
      <units>K</units>
      <id>tsfc</id>
      <rank>1</rank>
      <type>real</type>
      <description>ocean surface skin temperature</description>
      <kind>kind_phys</kind>
      <intent>out</intent>
      <optional>F</optional>
    </var>
  </subroutine>
</scheme>
