      <type>integer</type>
      <description>landmask: sea/land/ice=0/1/2</description>
      <kind />
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>orography</name>
      <units />
      <id>oro</id>
      <rank>1</rank>
      <type>real</type>
      <description>orography</description>
      <kind>kind_phys</kind>
      <intent>in</intent>
      <optional>F</optional>
    </var>
    <var>
      <name>orography_unfiltered</name>
      <units />
      <id>oro_uf</id>
      <rank>1</rank>
      <type>real</type>
